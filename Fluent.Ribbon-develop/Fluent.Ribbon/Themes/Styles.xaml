        <Setter Property="Width" Value="22" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="Fluent:RibbonProperties.IconSize" Value="Small" />
      </Trigger>
    </Style.Triggers>
  </Style>
