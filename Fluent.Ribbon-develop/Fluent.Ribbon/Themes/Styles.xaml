          </Grid.ColumnDefinitions>
          <TextBlock Text="{Binding Header, RelativeSource={RelativeSource TemplatedParent}}" TextWrapping="Wrap" VerticalAlignment="Center" Margin="5,0,0,0" x:Name="headerTextBlock" />
